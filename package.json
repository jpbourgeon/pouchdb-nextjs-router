{
  "name": "pouchdb-nextjs-router",
  "version": "1.0.0-beta.3",
  "description": "A next.js API submodule with a CouchDB style REST interface to PouchDB",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpbourgeon/pouchdb-nextjs-router.git"
  },
  "keywords": [
    "pouchdb",
    "next.js",
    "router"
  ],
  "author": "jpbourgeon <jeanphilippe.bourgeon@gmail.com> (https://github.com/jpbourgeon)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jpbourgeon/pouchdb-nextjs-router/issues"
  },
  "homepage": "https://github.com/jpbourgeon/pouchdb-nextjs-router",
  "main": "dist/index.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "next build",
    "build:commonjs": "rimraf dist && babel --plugins transform-remove-console lib --out-dir dist",
    "dev": "next dev",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix --cache",
    "lint:prettier": "prettier --write **/*.{js,jsx,ts,tsx}",
    "release": "standard-version",
    "start": "next start",
    "time:custom": "TIME=1 npm run test --prefix ../pouchdb",
    "time:express": "SERVER=express TIME=1 npm run test --prefix ../pouchdb",
    "time": "SERVER=pouchdb-nextjs-router TIME=1 npm run test --prefix ../pouchdb",
    "test:custom": "npm run test --prefix ../pouchdb",
    "test:express": "SERVER=express npm run test --prefix ../pouchdb",
    "test": "SERVER=pouchdb-nextjs-router npm run test --prefix ../pouchdb",
    "benchmark": "BENCHMARK=1 npm run test --prefix ../pouchdb"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "multiparty": "^4.2.2",
    "path-to-regexp": "^6.2.0"
  },
  "peerDependencies": {
    "next": "^10.0.0",
    "pouchdb": "^7.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@commitlint/config-conventional": "^11.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cors": "^2.8.5",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "helmet": "^4.2.0",
    "next": "10.0.3",
    "pouchdb": "^7.2.2",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0"
  }
}
